# Layout API
LAYOUT_API_URL=http://localhost:8080/layout-parsing
LAYOUT_API_TIMEOUT_SECONDS=30
LAYOUT_API_RETRIES=2
LAYOUT_API_BACKOFF_SECONDS=0.5
LAYOUT_API_FAIL_FAST=true

# OCI Object Storage (强依赖)
OCI_BUCKET_NAME=finance-bucket
OCI_REGION=us-ashburn-1
OCI_AUTH_METHOD=config_file
OCI_CONFIG_FILE=/root/.oci/config
OCI_CONFIG_PROFILE=DEFAULT

# Limits
MAX_UPLOAD_BYTES=52428800
MAX_PAGE_RANGE_PAGES=50

